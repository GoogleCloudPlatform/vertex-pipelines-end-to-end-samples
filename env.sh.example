export PAYLOAD=dev.json
export VERTEX_PROJECT_ID=<my-dev-project-id>

export PIPELINE_FILES_BUCKET_NAME=${VERTEX_PROJECT_ID}-pipeline-assets-bucket
export PIPELINE_ROOT_BUCKET_NAME=${VERTEX_PROJECT_ID}-pipeline-root-bucket
export VERTEX_SA_NAME=vertex-pipelines

export PIPELINE_FILES_GCS_PATH=gs://${PIPELINE_FILES_BUCKET_NAME}/pipelines
# pipeline template - update to any pipelines under the pipelines folder
# tensorflow or xgboost
export PIPELINE_TEMPLATE=xgboost
export VERTEX_CMEK_IDENTIFIER= # optional
export VERTEX_REGION=europe-west4
export VERTEX_NETWORK= # optional
export VERTEX_PIPELINE_ROOT=gs://${PIPELINE_ROOT_BUCKET_NAME}/pipeline_root
export VERTEX_SA_EMAIL=${VERTEX_SA_NAME}@${VERTEX_PROJECT_ID}.iam.gserviceaccount.com
